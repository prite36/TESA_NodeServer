<!DOCTYPE html>
<html>

<head>
  <% include head.ejs %>
</head>

<body>
  <header>
    <% include header.ejs %>
  </header>
  <main>
    <div class="jumbotron">
      <table>
        <% for(var i=0; i < data.sensors.length; i++) { %>
<td>
  <%= data.sensors[i].sensName %>
</td>
          <% for(var j=0; j < data.sensors[i].data.length; j++) { %>
            <tr>

              <td>
                <%= data.sensors[i].data[j].sensID %>
              </td>
              <td>
                <%= data.sensors[i].data[j].val %>
              </td>
              <td>
                <%= data.sensors[i].data[j].date %>
              </td>
            </tr>
          <% } %>
        <% } %>

      </table>
    </div>
  </main>
  <footer>
    <% include footer.ejs %>
  </footer>
</body>

</html>
